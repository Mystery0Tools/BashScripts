# 录制的流量文件存储目录，也是回放的目录
config_save_dir=archive
# 回放时打印详细的日志
config_print_reply_info_log=false
# 录制时监听的端口
config_listen_port=80
# 录制时分片文件大小
config_file_size_limit=256m
# 回放流量时的速度
config_replay_speed=100%
# 回放流量时的http输出url
config_output_http=http://localhost
# 日志记录文件目录
config_log=/var/log/gor
# 中间件可执行文件路径，单引号不能删除
config_middleware=middleware.sh
# 是否启用中间件
config_enable_middleware=false
# 退出 gor 时使用强制退出
config_force_kill_gor=true